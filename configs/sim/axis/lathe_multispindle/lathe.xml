<?xml version='1.0' encoding='UTF-8'?>
<pyvcp>
    <vbox>
    	<vbox>
    		<label>
    			<text>"Spindle 0 speed:"</text>
    		</label>
    		<bar>
    			<halpin>"spindle.0.speed"</halpin>
    			<max_>5000</max_>
    		</bar>
    		<hbox>
    			<led halpin="spindle.0.at-speed" on_color="yellow" off_color="black"/>
    			<label text="At Speed"/>
    		</hbox>
    	</vbox>
        <vbox>
            <label>
                <text>"Spindle 1 speed:"</text>
            </label>
            <bar>
                <halpin>"spindle.1.speed"</halpin>
                <max_>5000</max_>
            </bar>
            <hbox>
                <led halpin="spindle.1.at-speed" on_color="yellow" off_color="black"/>
                <label text="At Speed"/>
            </hbox>
        </vbox>
    </vbox>
</pyvcp>
